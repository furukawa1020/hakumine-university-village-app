{
  "name": "hakumine-university-village-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:netlify": "npm run build:clean && npm run build:verbose",
    "build:verbose": "next build --debug",
    "build:debug": "npm install && npm run build --verbose",
    "build:safe": "npm run build:clean && next build",
    "build:clean": "node -e \"const fs = require('fs'); try { fs.rmSync('.next', {recursive: true, force: true}); console.log('Cleaned .next directory'); } catch(e) { console.log('.next not found, skipping'); } try { fs.rmSync('out', {recursive: true, force: true}); console.log('Cleaned out directory'); } catch(e) { console.log('out not found, skipping'); }\"",
    "prebuild": "echo 'Starting build process...'",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next out node_modules/.cache"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@hookform/resolvers": "^5.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "firebase": "^10.14.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "next": "15.4.4",
    "next-pwa": "^4.0.0-beta.0",
    "postcss": "^8.4.47",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-leaflet": "^5.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "18.19.1",
    "npm": ">=10.2.4"
  }
}
